{
  "productName": "electron-updater-private",
  "appId": "com.example.yourapp",
  "directories": {
    "output": "build/${version}"
  },
  "publish": [
    {
      "provider": "github",
      "private": true,
      "owner": "namikarif",
      "repo": "electron",
      "token": "aa279e1811b40bc90b52f4071285b0f43cff920e"
    }
  ],
  "files": [
    "dist/electron/**/*"
  ],
  "dmg": {
    "contents": [
      {
        "x": 410,
        "y": 150,
        "type": "link",
        "path": "/Applications"
      },
      {
        "x": 130,
        "y": 150,
        "type": "file"
      }
    ]
  },
  "mac": {
    "icon": "build/icons/icon.icns"
  },
  "win": {
    "icon": "build/icons/icon.ico"
  },
  "linux": {
    "icon": "build/icons"
  }
}
